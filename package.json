{
  "name": "movies",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyse:circular": "npx dpdm 'src/**/*.*' --exclude node_modules --tree false --warning false --exit-code circular:1",
    "analyse:deps": "npx depcheck",
    "dev": "NEXT_PRIVATE_LOCAL_WEBPACK=true next dev",
    "dev:windows": "set NEXT_PRIVATE_LOCAL_WEBPACK=true && next dev",
    "format": "prettier --config .prettierrc.yaml \"**/*.{js,jsx,ts,tsx,md,graphql}\" --write",
    "format:check": "prettier --config .prettierrc.yaml \"**/*.{js,jsx,ts,tsx,md,graphql}\" --check",
    "format:test": "prettier --config .prettierrc.yaml \"**/*.{js,jsx,ts,tsx,md,graphql}\" -l",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --cache --ext js,ts,tsx",
    "lint:fix": "eslint . --fix --cache --ext js,ts,tsx",
    "test": "jest --runInBand --silent",
    "test:local": "jest --maxWorkers=50%",
    "test:ci": "jest --runInBand --coverage --silent",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:coverage": "pnpm test --coverage --detectOpenHandles",
    "tsc": "pnpm tsc:check",
    "tsc:check": "tsc --noEmit --incremental",
    "tsc:emit": "tsc --declaration --emitDeclarationOnly --outDir",
    "tsc:watch": "tsc --noEmit --incremental --watch",
    "validate": "pnpm run format:check && pnpm run lint:fix && pnpm run tsc:check && pnpm run analyse:circular && pnpm run test:local"
  },
  "dependencies": {
    "@effector/next": "^0.7.2",
    "@farfetched/core": "^0.14.0",
    "@farfetched/zod": "^0.14.0",
    "clsx": "^2.1.1",
    "effector": "^23.4.4",
    "effector-react": "^23.3.0",
    "jest-extended": "^7.0.0",
    "next": "16.1.2",
    "rambdax": "^11.3.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-typescript": "^7.28.5",
    "@faker-js/faker": "^9.9.0",
    "@next/bundle-analyzer": "^16.1.2",
    "@next/env": "^16.1.2",
    "@swc/cli": "^0.7.10",
    "@swc/core": "^1.15.8",
    "@swc/helpers": "^0.5.18",
    "@swc/jest": "^0.2.39",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-jest": "^29.7.0",
    "dpdm": "^3.14.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-mock-fetch": "^2.0.5",
    "jest-to-match-shape-of": "^1.3.2",
    "jsdom-testing-mocks": "^1.16.0",
    "prettier": "^3.8.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "pnpm": "^10.2.0",
    "node": "^24.0.0"
  },
  "volta": {
    "pnpm": "10.28.0",
    "node": "24.11.0"
  }
}
